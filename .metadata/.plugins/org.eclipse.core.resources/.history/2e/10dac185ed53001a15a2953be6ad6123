
public class IntegerClass {
	
	public static void main(String[] args) {
		int x = 25;
		Integer y = new Integer(3);
		Integer z = new Integer("33");
		Integer a1 = new Integer("");
		
		System.out.println(x);
		System.out.println(y.toString());
		System.out.println(z);
		
		Integer a = 4;
		float c = y;
	}

}
